    "import json\n",
    "\n",
    "with open('yelpThaiFood.json') as file:\n",
    "    thailist = json.load(file)\n",
    "\n",
    "list_of_thai_res = []\n",
    "\n",
    "for x in thailist:\n",
    "    if ((x[\"rating\"] > 3.5) & (x['distance'] < 1000)) :\n",
    "        list_of_thai_res.append(x[\"name\"])\n",
    "\n",
    "print (list_of_thai_res)"
   ]
